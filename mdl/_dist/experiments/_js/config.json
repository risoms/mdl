//jsdoc
//to run [cmd]: jsdoc -c config.json
//guide: http://usejsdoc.org/about-configuring-jsdoc.html
//guide: https://github.com/shri/JSDoc-Style-Guide
{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["src/js/gaze.js", "src/js/utils.js", "src/js/plugins/dotprobe-task.js", "src/js/plugins/camera-coordinates.js", "src/js/plugins/camera-calibration.js", "docs/links.json", "docs/directory.json"],
		"exclude": [],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"recurse": true,
		"private": true
	}
}

 